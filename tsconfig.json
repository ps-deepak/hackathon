{
  "compilerOptions": {
      "jsx": "react",
      "target": "es2016",                                 
      "module": "commonjs",                                
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,           
      "skipLibCheck": true,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": false
  }
}